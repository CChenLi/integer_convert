create_path <- function(x) {
suit <- as.character(x["suite_id"])
sam <- as.character(x["sample_id"])
code <- as.character(x["code"])
res <- paste(suit, "_", sam, "_", code, ".jpg", sep="")
sentenceString = gsub(" ","",res)
return(sentenceString)
}
